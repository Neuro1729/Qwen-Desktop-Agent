        "import os\n",
        "import threading\n",
        "\n",
        "# 1. Kill the old tunnel first\n",
        "!pkill cloudflared\n",
        "\n",
        "# 2. Define the background function\n",
        "def run_tunnel():\n",
        "    # This starts the tunnel and sends logs to a file instead of the screen\n",
        "    os.system(\"cloudflared tunnel --url http://127.0.0.1:8000 > tunnel.log 2>&1\")\n",
        "\n",
        "# 3. Start the thread\n",
        "threading.Thread(target=run_tunnel, daemon=True).start()\n",
        "\n",
        "print(\"Tunnel is now running in the background on port 8000.\")\n",
        "print(\"Check 'tunnel.log' file in the sidebar to see your URL!\")"
      ],
